<service name="TowServicePar3" scope="application" targetNamespace="http://ws.apache.org/axis2/ttp">
    <description>
        This service is to get the running Axis version
    </description>
    <parameter name="ServiceClass">TowServicePar3</parameter>
    <schema schemaNamespace="http://ws.apache.org/axis2/xsd/ttp"/>
    <operation name="tow">
        <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
    </operation>
    <operation name="pickUp">
        <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
    </operation>
    <operation name="deliver">
        <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
    </operation>
    <operation name="payTow">
        <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
    </operation>
    <operation name="getInsuranceFacts">
        <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
    </operation>
    <operation name="setInsuranceFacts">
        <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
    </operation>
</service>