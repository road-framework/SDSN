            </tns1:Global>
        </tns1:InterCollaborationRegulationUnit>
        <tns1:InterCollaborationRegulationUnit id="ruActivateV2">
            <tns1:Global>
                <tns1:RuleRef id="activateV2"/>
            </tns1:Global>
        </tns1:InterCollaborationRegulationUnit>
        <tns1:InterCollaborationRegulationUnit id="ruTerminateV1">
            <tns1:Global>
                <tns1:RuleRef id="terminateV1"/>
            </tns1:Global>
        </tns1:InterCollaborationRegulationUnit>
        <tns1:InterCollaborationRegulationUnit id="ruTerminateV2">
            <tns1:Global>
                <tns1:RuleRef id="terminateV2"/>
            </tns1:Global>
        </tns1:InterCollaborationRegulationUnit>
        <tns1:InterCollaborationRegulationUnit id="ruSwiftTowAndMacRepair">
            <tns1:Passthrough>
                <tns1:RuleRef id="orderRepair" place="MM-GC1"/>
                <tns1:RuleRef id="sendGCLocation" place="GC1-TC1"/>
                <tns1:RuleRef id="sendGCLocationResponse" place="GC1-TC1"/>
            </tns1:Passthrough>
            <tns1:Synchronization>
                <tns1:RuleRef id="deliverGC1" place="TC1"/>
                <tns1:RuleRef id="orderRepair" place="GC1"/>
            </tns1:Synchronization>
            <tns1:Routing>
                <tns1:RuleRef id="deliverResponseV2" place="TC1"/>
                <tns1:RuleRef id="requestAssistV3" place="MM"/>
                <tns1:RuleRef id="orderRepairResponse" place="GC1"/>
            </tns1:Routing>
        </tns1:InterCollaborationRegulationUnit>
        <tns1:InterCollaborationRegulationUnit id="ruSwiftTowAndAutoRepair">
            <tns1:Passthrough>
                <tns1:RuleRef id="orderRepair" place="MM-GC2"/>
                <tns1:RuleRef id="sendGCLocation" place="GC2-TC1"/>
                <tns1:RuleRef id="sendGCLocationResponse" place="GC2-TC1"/>
            </tns1:Passthrough>
            <tns1:Synchronization>
                <tns1:RuleRef id="deliverGC2" place="TC1"/>
                <tns1:RuleRef id="assessRepairTC1" place="GC2"/>
                <tns1:RuleRef id="orderRepairFromMM" place="GC2"/>
            </tns1:Synchronization>
            <tns1:Routing>
                <tns1:RuleRef id="deliverResponseV3" place="TC1"/>
                <tns1:RuleRef id="orderRepairResponseV2" place="GC2"/>
                <tns1:RuleRef id="requestAssistV4" place="MM"/>
            </tns1:Routing>
        </tns1:InterCollaborationRegulationUnit>
        <tns1:InterCollaborationRegulationUnit id="ruTomTowAndAutoRepair">
            <tns1:Passthrough>
                <tns1:RuleRef id="orderRepair" place="SC-GC2"/>
                <tns1:RuleRef id="sendGCLocation" place="GC2-TC2"/>
                <tns1:RuleRef id="sendGCLocationResponse" place="GC2-TC2"/>
            </tns1:Passthrough>
            <tns1:Synchronization>
                <tns1:RuleRef id="assessRepairTC2" place="GC2"/>
                <tns1:RuleRef id="orderRepair" place="GC2"/>
            </tns1:Synchronization>
            <tns1:Routing>
                <tns1:RuleRef id="orderRepairResponseV1" place="GC2"/>
                <tns1:RuleRef id="analyzeResponseV4" place="SC"/>
                <tns1:RuleRef id="towResponse" place="TC2"/>
            </tns1:Routing>
        </tns1:InterCollaborationRegulationUnit>
    </InterCollaborationRegulationUnits>
    <CollaborationUnits>
        <tns1:CollaborationUnit id="bCaseHandling">
            <tns1:ConfigurationDesign>
                <tns1:TaskRef Id="MM.RequestAssist"
                              postEP=" ( eAssistReqd ) | ( eAssistReqd * ( eRepairReqFromGC2ByMM  | eRepairReqFromGC1ByMM ) * ePickUpReq ) "/>
                <tns1:TaskRef Id="SC.Analyze" preEP=" eAssistReqd "
                              postEP=" ( eRentVehicleReqd ) | ( eRentRoomReqd ) | ( eTowReqd  *  eRepairReqFromGC2BySC * eRentVehicleReqd )"/>
                <tns1:TaskRef Id="MM.Notify" preEP=" eMMNotify "/>
            </tns1:ConfigurationDesign>
            <tns1:RegulationDesign>
                <tns1:Passthrough>
                    <tns1:RuleRef id="assist" place="SC-MM"/>
                    <tns1:RuleRef id="assistResponse" place="SC-MM"/>
                    <tns1:RuleRef id="notifyRoomPayment" place="SC-MM"/>
                    <tns1:RuleRef id="notifyVehiclePayment" place="SC-MM"/>
                </tns1:Passthrough>
                <tns1:Synchronization>
                    <tns1:RuleRef id="analyze" place="SC"/>
                    <tns1:RuleRef id="notify" place="MM"/>
                </tns1:Synchronization>
                <tns1:Routing>
                    <tns1:RuleRef id="requestAssistV1" place="MM"/>
                    <tns1:RuleRef id="payGCResponseV3" place="SC"/>
                </tns1:Routing>
            </tns1:RegulationDesign>
        </tns1:CollaborationUnit>
        <tns1:CollaborationUnit id="bRentingVehicle">
            <tns1:ConfigurationDesign>
                <tns1:TaskRef Id="VC.RentVehicle" preEP=" eRentVehicleReqd " postEP=" eRentedVehicle "/>
                <tns1:TaskRef Id="VC.EndVehicleRent" preEP=" eEndVehicleRentReqd " postEP=" eEndedVehicleRent "/>
                <tns1:TaskRef Id="SC.PayVC" preEP=" ePayVCReqd " postEP=" eVCPaid "/>
            </tns1:ConfigurationDesign>
            <tns1:RegulationDesign>
                <tns1:Passthrough>
                    <tns1:RuleRef id="rentVehicle" place="SC-VC"/>
                    <tns1:RuleRef id="rentVehicleResponse" place="SC-VC"/>
                    <tns1:RuleRef id="endVehicleRent" place="SC-VC"/>
                    <tns1:RuleRef id="endVehicleRentResponse" place="SC-VC"/>
                    <tns1:RuleRef id="payVehicleRent" place="SC-VC"/>
                    <tns1:RuleRef id="payVehicleRentResponse" place="SC-VC"/>
                </tns1:Passthrough>
                <tns1:Synchronization>
                    <tns1:RuleRef id="rentVehicle" place="VC"/>
                    <tns1:RuleRef id="endVehicleRent" place="VC"/>
                    <tns1:RuleRef id="payVC" place="SC"/>
                </tns1:Synchronization>
                <tns1:Routing>
                    <tns1:RuleRef id="analyzeResponseV1" place="SC"/>
                    <tns1:RuleRef id="payGCResponseV5" place="SC"/>
                    <tns1:RuleRef id="payVCResponse" place="SC"/>
                    <tns1:RuleRef id="rentVehicleResponse" place="VC"/>
                    <tns1:RuleRef id="endVehicleRentResponse" place="VC"/>
                </tns1:Routing>
            </tns1:RegulationDesign>
        </tns1:CollaborationUnit>
    </CollaborationUnits>
